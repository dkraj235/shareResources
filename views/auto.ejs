<%- include("partials/header") -%>
<%- include("partials/chooseone") -%>
 
<h1 class="branchNotes-heading">Automobiles</h1>

 

<section class="resource-margin" >
<div class="row deskRow mobilerow">
    <% allAutoFiles.forEach(function(autoFiles) {  %>
      <div class="card mb-5 mx-2 col-sm-6 cshadow" style="width: 18rem; background-color:#DDDDDD;">
        <div class="card-body ">
        <h5 class="card-title"><%= autoFiles.fileTitle %></h5>
        <div class="">
          <a href="/uploads/<%= autoFiles.pdf%>"><img style="border-radius:100%;" class="pdf-image" src="images/dilip.jpg" alt="pdf_image"></a>
        </div>
      </div>
    </div>
    <% }) %>

  </div>
</section>

 
